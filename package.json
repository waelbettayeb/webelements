{
  "name": "elements-kit",
  "type": "module",
  "version": "0.0.1",
  "description": "A lightweight reactive UI library that transforms native HTMLElements into reactive components with signals. Ideal for framework-agnostic applications and web components.",
  "keywords": [
    "webcomponents",
    "custom-elements",
    "signals",
    "reactivity",
    "typescript",
    "library",
    "frontend",
    "builder",
    "html",
    "web"
  ],
  "author": "@waelbettayeb",
  "license": "MIT",
  "main": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/core.js",
      "types": "./dist/core.d.ts"
    },
    "./signals": {
      "import": "./dist/signals.js",
      "types": "./dist/signals.d.ts"
    },
    "./dom": {
      "import": "./dist/dom.js",
      "types": "./dist/dom.d.ts"
    }
  },
  "scripts": {
    "demo": "pnpx vite ./demo",
    "build": "tsdown"
  },
  "packageManager": "pnpm@10.26.2",
  "devDependencies": {
    "tsdown": "0.19.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "alien-signals": "^3.1.2"
  }
}
